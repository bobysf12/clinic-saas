    },
    "dob": {
      "type": "date",
      "required": true
    },
    "address": {
      "type": "text"
    },
    "phone": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "others"
      ]
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
