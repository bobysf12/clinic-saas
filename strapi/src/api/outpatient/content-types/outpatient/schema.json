{
  "kind": "collectionType",
  "collectionName": "outpatients",
  "info": {
    "singularName": "outpatient",
    "pluralName": "outpatients",
    "displayName": "Outpatient",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "patient": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::patient.patient"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "in_queue",
        "in_progress",
        "waiting_for_payment",
        "waiting_for_medicine",
        "done",
        "canceled_by_admin"
      ],
      "default": "in_queue"
    },
    "appointment_date": {
      "type": "datetime"
    },
    "patient_record": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::patient-record.patient-record"
    },
    "doctor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::doctor.doctor"
    },
    "organization": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organization.organization"
    }
  }
}
