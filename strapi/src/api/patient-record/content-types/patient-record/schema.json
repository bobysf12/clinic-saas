{
  "kind": "collectionType",
  "collectionName": "patient_records",
  "info": {
    "singularName": "patient-record",
    "pluralName": "patient-records",
    "displayName": "Patient Record",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "attachments": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "patient": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::patient.patient"
    },
    "patient_record_inventories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::patient-record-inventory.patient-record-inventory",
      "mappedBy": "patient_record"
    },
    "subjective": {
      "type": "richtext"
    },
    "objective": {
      "type": "richtext"
    },
    "assessment": {
      "type": "richtext"
    },
    "plan": {
      "type": "richtext"
    },
    "doctor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::doctor.doctor"
    },
    "organization": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organization.organization",
      "private": true
    },
    "blood_pressure": {
      "type": "string"
    },
    "pulse": {
      "type": "integer"
    },
    "temperature": {
      "type": "decimal"
    },
    "respiratory_rate": {
      "type": "integer"
    },
    "saturation": {
      "type": "integer"
    },
    "height": {
      "type": "decimal"
    },
    "weight": {
      "type": "decimal"
    },
    "icd_10s": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::icd-10.icd-10"
    },
    "evaluation": {
      "type": "richtext"
    },
    "patient_record_medical_treatments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::patient-record-medical-treatment.patient-record-medical-treatment",
      "mappedBy": "patient_record"
    },
    "medical_treatment_note": {
      "type": "text"
    }
  }
}
