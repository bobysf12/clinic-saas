{
  "kind": "collectionType",
  "collectionName": "inventories",
  "info": {
    "singularName": "inventory",
    "pluralName": "inventories",
    "displayName": "Inventory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal",
      "required": false
    },
    "unit_in_stock": {
      "type": "integer"
    },
    "qty_per_unit": {
      "type": "integer"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "inventory_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::inventory-type.inventory-type"
    },
    "organization": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organization.organization"
    }
  }
}
